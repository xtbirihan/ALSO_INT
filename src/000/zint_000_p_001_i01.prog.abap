MODULE user_command_9000 INPUT.
  DATA(lv_ucomm) = okcode.
  CLEAR: okcode.
  CASE lv_ucomm.
    WHEN 'BACK' OR 'CANCEL' OR 'EXIT'.
      LEAVE TO SCREEN 0.
    WHEN OTHERS.
  ENDCASE.
ENDMODULE.
